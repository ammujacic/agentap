{
  "name": "@agentap-dev/adapter-claude-code",
  "version": "0.1.0",
  "description": "Claude Code adapter for Agentap",
  "agentap": {
    "type": "adapter",
    "name": "claude-code"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@agentap-dev/adapter-base": "workspace:*",
    "@agentap-dev/acp": "workspace:*",
    "chokidar": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
