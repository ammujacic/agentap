{
  "name": "@agentap-dev/cli",
  "version": "0.1.0",
  "description": "Agentap CLI daemon - bridge your AI coding agents to mobile",
  "bin": {
    "agentap": "./bin/agentap.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/cli.ts --format esm --watch --onSuccess 'API_URL=http://localhost:8787 PORTAL_URL=http://localhost:3001 node dist/cli.mjs start --no-tunnel'",
    "start": "node dist/cli.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@agentap-dev/acp": "workspace:*",
    "@agentap-dev/adapter-base": "workspace:*",
    "@agentap-dev/adapter-claude-code": "workspace:*",
    "@agentap-dev/adapter-opencode": "workspace:*",
    "@agentap-dev/shared": "workspace:*",
    "@iarna/toml": "^3.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "commander": "^14.0.3",
    "hono": "^4.0.0",
    "qrcode-terminal": "^0.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/ws": "^8.5.10",
    "eslint": "^9.39.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
