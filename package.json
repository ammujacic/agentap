{
  "name": "agentap",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile bridge for AI coding agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentap-dev/agentap.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "turbo dev",
    "dev:api": "turbo dev --filter=@agentap-dev/api",
    "dev:website": "turbo dev --filter=@agentap-dev/website",
    "dev:portal": "turbo dev --filter=@agentap-dev/portal",
    "dev:daemon": "turbo dev --filter=@agentap-dev/cli",
    "dev:mobile": "cd packages/mobile && npx expo start",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "typecheck": "turbo typecheck",
    "prepare": "husky",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "test:integration": "cd tests && pnpm test",
    "docker:up": "docker compose up -d",
    "docker:up:backend": "docker compose up -d api",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:reset": "docker compose down -v && docker compose up -d",
    "docker:test": "docker compose --profile test up --abort-on-container-exit tests",
    "docker:build": "docker compose build",
    "db:migrate": "cd apps/api && wrangler d1 execute agentap-db --local --file=./migrations/0001_initial.sql && wrangler d1 execute agentap-db --local --file=./migrations/0002_password_auth.sql",
    "db:seed": "cd apps/api && wrangler d1 execute agentap-db --local --file=./scripts/seed.sql",
    "db:reset": "rm -rf apps/api/.wrangler && pnpm db:migrate && pnpm db:seed"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react-server-dom-webpack>react": "19.1.0",
        "react-server-dom-webpack>react-dom": "19.1.0"
      }
    }
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,css,scss,yaml,yml}": "prettier --write"
  }
}
